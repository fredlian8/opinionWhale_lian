{
  "version": 2,
  "builds": [
    {
      "src": "api/markets.py",
      "use": "@vercel/python",
      "config": {
        "maxDuration": 60
      }
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/markets",
      "dest": "/api/markets.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
 
}
